{
  "name": "pdfactory",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/pdfactory.umd.cjs",
  "exports": {
    ".": {
      "require": "./dist/pdfactory.umd.cjs"
    }
  },
  "version": "0.0.2",
  "author": "PdFactory",
  "scripts": {
    "build": "npx rollup --config ./config/rollup.config.js",
    "dev": "nodemon --watch build --watch src --watch test -e js,ts,json,ejs,css,html ./test/dev.js",
    "dev:watch": "concurrently \"yarn clean-build\" \"yarn typescript:watch\" \"wait-on ./build/main.js && yarn dev\"",
    "clean-build": "rimraf ./build",
    "typescript:watch": "npx tsc -build config/tsconfig-local.json -w",
    "test-build": "node ./test/testBuild.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-eslint": "^9.0.1",
    "@rollup/plugin-json": "^5.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.14",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "typescript": "^4.9.4",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "ejs": "^3.1.8",
    "lodash.merge": "^4.6.2",
    "puppeteer": "19.4.1"
  },
  "engines": {
    "node": "16.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/maxcaron/pdfactory.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxcaron/pdfactory/issues"
  },
  "homepage": "https://https://github.com/maxcaron/pdfactory"
}
