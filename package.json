{
  "author": "PdFactory",
  "name": "pdfactory",
  "version": "0.0.25",
  "type": "module",
  "files": [
    "dist/pdfactory.d.ts",
    "dist/pdfactory.umd.cjs",
    "dist/types.d.ts",
    "dist/.local-chromium"
  ],
  "types": "./dist/pdfactory.d.ts",
  "main": "./dist/pdfactory.umd.cjs",
  "exports": {
    ".": {
      "types": "./dist/pdfactory.d.ts",
      "require": "./dist/pdfactory.umd.cjs"
    }
  },
  "scripts": {
    "dist": "rimraf ./dist && npx rollup --config ./config/rollup.config.js",
    "dev": "DEV=true concurrently \"yarn dev:watch\" \"wait-on ./build/bundle.js && nodemon --inspect --watch build --watch test ./test/dev.js\"",
    "dev:watch": "rimraf ./build && npx rollup -w -c ./config/rollup.config.js",
    "test-build": "node ./test/testBuild.js"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/express": "^4.17.14",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.7.18",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "typescript": "^4.9.4",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@types/ejs": "^3.1.1",
    "ejs": "^3.1.8",
    "lodash.merge": "^4.6.2",
    "puppeteer": "19.4.1",
    "puppeteer-core": "^19.5.0"
  },
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/maxcaron/pdfactory.git"
  },
